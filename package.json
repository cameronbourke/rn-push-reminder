{
  "name": "rn-push-reminder",
  "description": "Inspired by the modal used in Facebook's messenger app to remind the user to enable push notifications, is a react native component that lets you easily add the same reminder to your iOS app.",
  "version": "0.0.2",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "react-component",
    "modal",
    "push-notifications",
    "react-native",
    "ios"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronbourke/rn-push-reminder.git"
  },
  "scripts": {
    "compile": "babel src/push-reminder.js --out-file dist/index.js",
    "compile:watch": "babel src/push-reminder.js --watch --out-file example/app/push-reminder.js",
    "prepublish": "npm run compile"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "~6.3.26",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13"
  },
  "peerDependencies": {
    "react-native": "^0.19.0"
  }
}
